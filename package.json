        "serve:stg:ssr": "PORT=4003 node dist/app/server/main.js",
        "build:prd:ssr": "ng build --configuration=production && ng run app:server:production",
        "serve:prd:ssr": "PORT=4004 node dist/app/server/main.js",
        "prerender": "ng run app:prerender"
    },
    "private": true,
